{
  "manifest_version": 3,
  "name": "Cookie Copy Paster",
  "version": "1.0.0",
  "description": "Копирует выбранные cookie с одного URL на другой.",
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cookie Copy Paster"
  }
}
